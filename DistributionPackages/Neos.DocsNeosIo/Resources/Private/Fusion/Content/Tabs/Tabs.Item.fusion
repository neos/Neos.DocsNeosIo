prototype(Neos.DocsNeosIo:Content.Tabs.Item) < prototype(Neos.Neos:ContentComponent) {
    content = Neos.Neos:ContentCollection {
        nodePath = '.'
    }

    renderer = afx`
        <div id={'panel-' + node.nodeAggregateId.value} class="tab-item" role="tabpanel" tabindex="0" aria-labelledby={'tab-' + node.nodeAggregateId.value}>
            {props.content}
        </div>
    `
}
