{
    "name": "neos/docs-neos-io-distribution",
    "description": "docs.neos.io website distribution",
    "license": "GPL-3.0-or-later",
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin",
        "allow-plugins": {
            "neos/composer-plugin": true,
            "cweagans/composer-patches": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "neos/docs-neos-io": "@dev",
        "neos/contentrepository-legacynodemigration": "^9.0",
        "sandstorm/lazydatasource": "dev-feature/neos-9 as 3.0.0",
        "jonnitto/prettyembedhelper": "dev-neos9 as 7.0.0",
        "flownative/google-cloudstorage": "^6.1",
        "rokka/imagine-vips": "^0.31.0",
        "jcupitt/vips": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "neos/rector": "dev-main",
        "phpstan/phpstan": "^1.10"
    },
    "notCompatible": {
        "shel/neos-terminal": "dev-neos9"
    },
    "repositories": {
        "distribution": {
            "type": "path",
            "url": "./DistributionPackages/*"
        },
        "Jan3k3y/neos-matomo": {
            "type": "git",
            "url": "https://github.com/Jan3k3y/neos-matomo.git"
        },
        "markusguenther/lazydatasource": {
            "type": "git",
            "url": "https://github.com/markusguenther/LazyDataSource.git"
        },
        "markusguenther/mxgraph": {
            "type": "git",
            "url": "https://github.com/markusguenther/MxGraph.git"
        },
        "Jan3k3y/neos-publishnotifier": {
            "type": "git",
            "url": "https://github.com/Jan3k3y/neos-publishnotifier.git"
        },
        "markusguenther/prettyembedhelper": {
            "type": "git",
            "url": "https://github.com/markusguenther/Jonnitto.PrettyEmbedHelper.git"
        },
        "markusguenther/prettyembedvideoplatforms": {
            "type": "git",
            "url": "https://github.com/markusguenther/Jonnitto.PrettyEmbedVideoPlatforms.git"
        }
    },
    "scripts": {
        "post-update-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-package-update": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    }
}
