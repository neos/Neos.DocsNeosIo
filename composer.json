{
    "name": "neos/docs-neos-io-distribution",
    "description": "docs.neos.io website distribution",
    "license": "GPL-3.0-or-later",
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin",
        "allow-plugins": {
            "neos/composer-plugin": true,
            "cweagans/composer-patches": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "neos/docs-neos-io": "@dev",
        "neos/contentrepository-legacynodemigration": "9.0.*@beta",

        "flownative/google-cloudstorage": "v6.0.0-beta.4",
        "rokka/imagine-vips": "^0.31.0",
        "jcupitt/vips": "^1.0",

        "cweagans/composer-patches": "^1.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "neos/rector": "dev-main",
        "phpstan/phpstan": "^1.10"
    },
    "notCompatible": {
        "shel/neos-terminal": "dev-neos9"
    },
    "repositories": {
        "distribution": {
            "type": "path",
            "url": "./DistributionPackages/*"
        },
        "queo/carbonEel": {
            "type": "git",
            "url": "https://github.com/queoGmbH/Carbon.Eel.git"
        },
        "queo/carbonIncludeAssets": {
            "type": "git",
            "url": "https://github.com/queoGmbH/Carbon.IncludeAssets.git"
        },
        "so-grimm/carbonLink": {
            "type": "git",
            "url": "https://github.com/so-grimm/Carbon.Link.git"
        },
        "queo/content-repository-search": {
            "type": "git",
            "url": "https://github.com/queoGmbH/content-repository-search.git"
        },
        "queo/lazydatasource": {
            "type": "git",
            "url": "https://github.com/queoGmbH/LazyDataSource.git"
        },
        "queo/mxgraph": {
            "type": "git",
            "url": "https://github.com/queoGmbH/MxGraph.git"
        },
        "Jan3k3y/neos-matomo": {
            "type": "git",
            "url": "https://github.com/Jan3k3y/neos-matomo.git"
        },
        "queo/neos-terminal": {
            "type": "git",
            "url": "https://github.com/queoGmbH/Shel.Neos.Terminal.git"
        },
        "Jan3k3y/neos-publishnotifier": {
            "type": "git",
            "url": "https://github.com/Jan3k3y/neos-publishnotifier.git"
        },
        "queo/prettyembedhelper": {
            "type": "git",
            "url": "https://github.com/queoGmbH/Jonnitto.PrettyEmbedHelper.git"
        },
        "queo/prettyembedvideoplatforms": {
            "type": "git",
            "url": "https://github.com/queoGmbH/Jonnitto.PrettyEmbedVideoPlatforms.git"
        },
        "queo/searchplugin": {
            "type": "git",
            "url": "https://github.com/queoGmbH/Flowpack.SearchPlugin.git"
        },
        "queo/yoastSEOForNeos": {
            "type": "git",
            "url": "https://github.com/queoGmbH/Yoast-SEO-for-Neos.git"
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patches": {
            "neos/contentrepository-nodeaccess": {
                "TASK: Reintroduce 8.3 context fq operation": "Patches/5130.patch"
            }
        }
    },
    "scripts": {
        "post-update-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-package-update": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    }
}
